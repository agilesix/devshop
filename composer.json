{
    "name": "devshop/devmaster",
    "description": "Web Dashboard for DevShop",
    "type": "project",
    "keywords": ["framework", "devops", "platform", "hosting"],
    "homepage": "https://getdevshop.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Pugh",
            "email": "jon@thinkdrop.net",
            "role": "Author"
        },
        {
            "name": "DevShop Community",
            "homepage": "https://github.com/opendevshop/devshop/graphs/contributors"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/7"
        }
    ],
    "require": {
        "knplabs/github-api": "^1.4",
        "cpliakas/git-wrapper": "2.*",
        "php": "^7.1",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-xml": "*",
        "asm/php-ansible": "^2.0",
        "composer/installers": "^1.2",
        "composer/semver": "^1.4",
        "consolidation/robo": "^2",
        "cpliakas/git-wrapper": "1.*",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/preserve-paths": "^0.1",
        "drupal/admin_menu": "^3.0@RC",
        "drupal/adminrole": "^1.1",
        "drupal/aegir_ansible": "^1.0@beta",
        "drupal/aegir_cloud": "^1.0@beta",
        "drupal/aegir_config": "^1.0@beta",
        "drupal/aegir_ssh": "^1.0",
        "drupal/betterlogin": "^1.5",
        "drupal/bootstrap": "^3.26",
        "drupal/cas": "^1.7",
        "drupal/chosen": "^2.1",
        "drupal/composer_autoloader": "^1.0",
        "drupal/ctools": "^1.15",
        "drupal/devel": "^1.7",
        "drupal/distro_update": "^1.0@beta",
        "drupal/drupal": "^7.62",
        "drupal/eldir": "^3.180",
        "drupal/entity": "^1.9",
        "drupal/features": "^2.11",
        "drupal/hosting": "^3.180",
        "drupal/hosting_git": "^3.181",
        "drupal/hosting_https": "^3.182",
        "drupal/hosting_logs": "^3.181",
        "drupal/hosting_statsd": "^1.0@beta",
        "drupal/hosting_tasks_extra": "^3.180",
        "drupal/hybridauth": "^2.16",
        "drupal/jquery_update": "^2.7",
        "drupal/libraries": "^2.5",
        "drupal/module_filter": "^2.2",
        "drupal/navbar": "^1.7",
        "drupal/openidadmin": "^1.0",
        "drupal/overlay_paths": "^1.3",
        "drupal/r4032login": "^1.8",
        "drupal/sshkey": "^2.0",
        "drupal/statsd": "^1.1",
        "drupal/timeago": "^2.3",
        "drupal/token": "^1.7",
        "drupal/views": "^3.23",
        "drupal/views_bulk_operations": "^3.5",
        "drush/drush": "^8.0",
        "knplabs/github-api": "^1.4",
        "mouf/nodejs-installer": "^1.0",
        "provision-ops/power-process": "^1.2",
        "symfony/filesystem": "~2.7|^3",
        "webflo/drupal-finder": "^1.0.0"
    },
    "conflict": {
        "drupal/core": "8.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "scripts/devshop",
        "scripts/branch-or-tag"
    ],
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.1.3"
        },
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "psr-0": { "DevShop": "src/" }
    },
    "scripts": {
        "bin-install": [
            "DevShop\\Composer::installBins"
        ],
        "split": [
            "DevShop\\Composer::installBins",
            "DevShop\\Composer::splitRepos"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DevShop\\Composer::installBins",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DevShop\\Composer::installBins",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-create-project-cmd": [
            "DrupalProject\\composer\\ScriptHandler::removeInternalFiles"
        ],
        "update-roles": [
            "DevShop\\Composer::updateRoles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/": ["type:drupal-core"],
            "web/profiles/{$name}/": ["type:drupal-profile"],
            "web/sites/all/drush/{$name}/": ["type:drupal-drush"],
            "web/sites/all/libraries/{$name}/": ["type:drupal-library"],
            "web/sites/all/modules/contrib/{$name}/": ["type:drupal-module"],
            "web/sites/all/themes/contrib/{$name}/": ["type:drupal-theme"]
        },
        "patches": {
            "cweagans/composer-patches": {
                "Call the preserve paths hooks" : "https://github.com/SebCorbin/composer-patches/commit/1cb9bacba51f8bba0c5f3f322c3bb61e4ceae974.patch"
            }
        },
        "preserve-paths": [
            "web/sites"
        ]
    }
}
